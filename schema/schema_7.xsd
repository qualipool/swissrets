      <xs:enumeration value="area_sia_gf"/> <!-- (Brutto) Geschossfläche -->
      <xs:enumeration value="area_sia_ngf"/> <!-- Nettogeschossfläche -->
      <xs:enumeration value="area_sia_kf"/> <!-- Konstruktionsfläche -->
      <xs:enumeration value="area_sia_nf"/> <!-- Nutzfläche -->
      <xs:enumeration value="area_sia_vf"/> <!-- Verkehrsfläche -->
      <xs:enumeration value="area_sia_ff"/> <!-- Funktionsfläche -->
      <xs:enumeration value="area_sia_kft"/> <!-- Konstruktionsfläche Tragend -->
      <xs:enumeration value="area_sia_kfn"/> <!-- Konstruktionsfläche nicht Tragend -->
      <xs:enumeration value="area_sia_hnf"/> <!-- Hauptnutzfläche -->
      <xs:enumeration value="area_sia_nnf"/> <!-- Nebennutzfläche -->
      
      <!-- DIN 277 -->
      <!--<xs:enumeration value="area_din_bgf"/>-->
      <!--<xs:enumeration value="area_din_kgf"/>-->
      <!--<xs:enumeration value="area_din_ngf"/>-->
      <!--<xs:enumeration value="area_din_nf"/>-->
      <!--<xs:enumeration value="area_din_tf"/>-->
      <!--<xs:enumeration value="area_din_vf"/>-->
      
