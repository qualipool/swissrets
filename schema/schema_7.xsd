      <xs:enumeration value="construction_utilization_number"/> <!--Ausnützungsziffer (bau)-->
      
      <xs:enumeration value="year_built"/>
      <xs:enumeration value="year_last_renovated"/>
      <xs:enumeration value="ceiling_height"/>
      <xs:enumeration value="hall_height"/>
      <xs:enumeration value="maximal_floor_loading"/> <!-- m2 -->
      <xs:enumeration value="carrying_capacity_crane"/> <!-- depricated -->
      <xs:enumeration value="carrying_capacity_elevator"/>


    
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="relationkeys">
    <xs:restriction base="xs:string">
      <xs:enumeration value="closeProximity"/>
      <xs:enumeration value="child"/>
      <xs:enumeration value="parent"/>
      <xs:enumeration value="similar"/>
    </xs:restriction>
  </xs:simpleType>

  <xs:simpleType name="emailAddress"> 
    <xs:restriction base="xs:string"> 
      <xs:pattern value="[^@]+@[^\.]+\..+"/> 
    </xs:restriction> 
  </xs:simpleType> 


  <xs:element name="export">
    <xs:complexType>
      <xs:all>
        <xs:element name="exportInformation" maxOccurs="1" minOccurs="1">
          <xs:complexType>
            <xs:all>
              <xs:element type="xs:string" name="exporter" minOccurs="0" maxOccurs="1"/>
              <xs:element type="xs:dateTime" name="exportDate" minOccurs="0" maxOccurs="1"/>
              <xs:element type="xs:integer" name="exportIteration" default="1" minOccurs="0" maxOccurs="1"/>
            </xs:all>
          </xs:complexType>
        </xs:element>
        <xs:element name="properties" maxOccurs="1" minOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="property" maxOccurs="unbounded" minOccurs="1">
                <xs:complexType>
                  <xs:all>
                    <xs:element type="xs:string" name="zoneTypes" minOccurs="0" maxOccurs="1"/>
                    <xs:element type="xs:string" name="parcelNumbers" minOccurs="0" maxOccurs="1"/>
                    <xs:element type="xs:string" name="referenceId" minOccurs="0" maxOccurs="1"/>
                    <xs:element type="xs:string" name="type" minOccurs="1" maxOccurs="1"/>
                    <xs:element type="availability" name="availability" minOccurs="1" maxOccurs="1"/>
                    <xs:element type="xs:dateTime" name="start" minOccurs="0" maxOccurs="1"/>
                    <xs:element type="currencyCodeType" name="priceCurrency" minOccurs="1" maxOccurs="1"/>
                    <xs:element type="price" name="price" minOccurs="0" maxOccurs="1"/>
                    <xs:element type="price" name="netPrice" minOccurs="0" maxOccurs="1"/>
                    <xs:element type="price" name="grossPrice" minOccurs="0" maxOccurs="1"/>
                    <xs:element name="extraCosts" minOccurs="0" maxOccurs="1">
                    <xs:complexType>
                      <xs:sequence>
                        <xs:element name="extraCost" minOccurs="1" maxOccurs="unbounded">
                          <xs:complexType>
                            <xs:simpleContent>
                              <xs:extension base="xs:short">
                                <xs:attribute type="xs:string" name="key" use="optional"/>
                                <xs:attribute type="xs:byte" name="frequency" use="optional"/>
                                <xs:attribute type="xs:string" name="timesegment" use="optional"/>
                                <xs:attribute type="xs:string" name="propertysegment" use="optional"/>
                                <xs:attribute type="xs:string" name="type" use="optional"/>
                                <xs:attribute type="xs:string" name="title" use="optional"/>
                              </xs:extension>
                            </xs:simpleContent>
                          </xs:complexType>
                        </xs:element>
                      </xs:sequence>
                    </xs:complexType>
                  </xs:element>
                    <xs:element name="publisherSettings" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="publisherSetting" minOccurs="0" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:all>
                                <xs:element type="xs:string" name="image_limit" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="xs:string" name="document_limit" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="xs:string" name="plan_limit" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="xs:string" name="hide_address" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="xs:string" name="hide_street" minOccurs="0" maxOccurs="1"/>
                                <xs:element name="options" minOccurs="0" maxOccurs="1">
                                  <xs:complexType mixed="true">
                                    <xs:sequence>
                                      <xs:element name="option" maxOccurs="unbounded" minOccurs="1">
                                      <xs:complexType>
                                      <xs:simpleContent>
                                      <xs:extension base="xs:string">
                                      <xs:attribute type="xs:string" name="key" use="optional"/>
                                      </xs:extension>
                                      </xs:simpleContent>
                                      </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                              </xs:all>
                              <xs:attribute type="xs:string" name="id" use="required"/>
                              <xs:attribute type="xs:string" name="label" use="optional"/>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="address" type="address" minOccurs="1" maxOccurs="1"/>
                    <xs:element name="softwareInformation" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="xs:string" name="lastUser" minOccurs="0" maxOccurs="1"/>
                          <xs:element type="xs:dateTime" name="lastUpdate" minOccurs="1" maxOccurs="1"/>
                          <xs:element type="xs:byte" name="updateIteration" minOccurs="0" maxOccurs="1"/>
                          <xs:element type="xs:dateTime" name="creation" minOccurs="0" maxOccurs="1"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="providerInformation" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="xs:boolean" name="public" minOccurs="0" maxOccurs="1"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="features" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="feature" type="featureTypes" maxOccurs="unbounded" minOccurs="1"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="numericValues" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="value" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute type="numericValuekeys" name="key" use="required"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="categories" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="categoryTypes" name="category" minOccurs="1" maxOccurs="unbounded"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="utilities" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element type="utilityTypes" name="utility" minOccurs="1" maxOccurs="unbounded"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="relations" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="relation" maxOccurs="unbounded" minOccurs="0">
                            <xs:complexType>
                              <xs:simpleContent>
                                <xs:extension base="xs:string">
                                  <xs:attribute type="relationkeys" name="key" use="required"/>
                                </xs:extension>
                              </xs:simpleContent>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="seller" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="organization" minOccurs="0" maxOccurs="1">
                            <xs:complexType>
                              <xs:all>
                                <xs:element name="address" type="address" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="xs:string" name="legalName" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="xs:string" name="brand" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="emailAddress" name="email" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="emailAddress" name="emailRem" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="xs:string" name="fax" minOccurs="0" maxOccurs="1"/>
                                <xs:element type="xs:string" name="phone" minOccurs="0" maxOccurs="1"/>
                                <xs:element name="website" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:simpleContent>
                                      <xs:extension base="xs:anyURI">
                                      <xs:attribute type="xs:string" name="title"/>
                                      <xs:attribute type="xs:string" name="label"/>
                                      </xs:extension>
                                    </xs:simpleContent>
                                  </xs:complexType>
                                </xs:element>
                              </xs:all>
                            </xs:complexType>
                          </xs:element>
                          <xs:element type="person" name="viewPerson" minOccurs="0" maxOccurs="1"/>
                          <xs:element type="person" name="visitPerson" minOccurs="0" maxOccurs="1"/>
                          <xs:element type="person" name="inquiryPerson" minOccurs="0" maxOccurs="1"/>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="details" minOccurs="1" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="detail" minOccurs="1" maxOccurs="unbounded">
                            <xs:complexType>
                              <xs:all>

                                <xs:element type="xs:string" name="name" minOccurs="1" maxOccurs="1"/>
                                <xs:element type="xs:string" name="excerpt" minOccurs="0" maxOccurs="1"/>
                                
                                <xs:element name="tags" maxOccurs="1" minOccurs="0">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="tag" minOccurs="0" maxOccurs="unbounded"/>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="urls" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element type="fullUrl" name="url" minOccurs="0" maxOccurs="unbounded"/>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="descriptions" maxOccurs="1" minOccurs="0">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="description" minOccurs="0" maxOccurs="unbounded">
                                        <xs:complexType>
                                          <xs:simpleContent>
                                            <xs:extension base="xs:string">
                                              <xs:attribute type="xs:string" name="title" use="optional"/>
                                            </xs:extension>
                                          </xs:simpleContent>
                                        </xs:complexType>
                                      </xs:element>
                                    </xs:sequence>
                                  </xs:complexType>
                                </xs:element>
                                <xs:element name="attachments" minOccurs="0" maxOccurs="1">
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="media" minOccurs="1" maxOccurs="unbounded">
