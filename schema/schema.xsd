            <xs:enumeration value="link"></xs:enumeration>
            <!-- a genneric link for display and hrefing. -->
            <xs:enumeration value="direct-link"></xs:enumeration>
            <!-- use as replacement link. Aka. a custom Endpoint webpage instead of a generated single/data view. -->
            <xs:enumeration value="virtual-tour"></xs:enumeration>
