<?xml version="1.0" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified">
    <!-- TYPE DEFFINITIONS START HERE -->
    <xs:complexType name="person">
        <xs:all>
            <xs:element type="xs:string" name="function" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="givenName" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="familyName" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="emailAddress" name="email" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="fax" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="phone" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="mobile" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="gender" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="note" minOccurs="0" maxOccurs="1"></xs:element>
        </xs:all>
    </xs:complexType>
    <xs:complexType name="address">
        <xs:all>
            <xs:element type="countryCodeType" name="country" minOccurs="1" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="locality" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="region" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="postalCode" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="postOfficeBoxNumber" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="street" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="streetNumber" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:string" name="streetAddition" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element type="xs:byte" name="subunit" minOccurs="0" maxOccurs="1"></xs:element>
            <xs:element name="geo" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                    <xs:all>
                        <xs:element type="xs:float" name="latitude" minOccurs="1" maxOccurs="1"></xs:element>
                        <xs:element type="xs:float" name="longitude" minOccurs="1" maxOccurs="1"></xs:element>
                        <xs:element type="xs:float" name="elevation" minOccurs="0" maxOccurs="1"></xs:element>
                    </xs:all>
                </xs:complexType>
            </xs:element>
        </xs:all>
    </xs:complexType>
    <xs:simpleType name="availability">
        <xs:restriction base="xs:string">
            <xs:enumeration value="reserved"></xs:enumeration>
            <xs:enumeration value="active"></xs:enumeration>
            <xs:enumeration value="taken"></xs:enumeration>
            <xs:enumeration value="reference"></xs:enumeration>
            <xs:enumeration value="private"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="propertyType">
        <xs:annotation>
            <xs:documentation>rent: Mieten, buy: Kaufen</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="rent"></xs:enumeration>
            <xs:enumeration value="buy"></xs:enumeration>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="price">
        <xs:annotation>
            <xs:documentation source="https://github.com/qualipool/swissrets/raw/master/docs/price.pdf"></xs:documentation>
        </xs:annotation>
        <xs:simpleContent>
            <xs:extension base="xs:positiveInteger">
                <xs:attribute type="xs:positiveInteger" name="frequency" use="optional" default="1"></xs:attribute>
                <xs:attribute name="timesegment" use="optional">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="infinite"></xs:enumeration>
                            <xs:enumeration value="d"></xs:enumeration>
                            <xs:enumeration value="w"></xs:enumeration>
                            <xs:enumeration value="m"></xs:enumeration>
                            <xs:enumeration value="y"></xs:enumeration>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="propertysegment" use="optional" default="all">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="all"></xs:enumeration>
                            <xs:enumeration value="m2"></xs:enumeration>
                            <xs:enumeration value="km2"></xs:enumeration>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
    <xs:complexType name="priceRange">
        <xs:all>
            <xs:element type="xs:positiveInteger" name="from" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
                "from" needs to be smaller than "to"
            </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element type="xs:positiveInteger" name="to" minOccurs="1" maxOccurs="1">
                <xs:annotation>
                    <xs:documentation xml:lang="en">
                "to" needs to be larger than "from"
            </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:all>
        <xs:attribute name="propertysegment" use="optional">
            <xs:simpleType>
                <xs:restriction base="xs:string">
                    <xs:enumeration value="all"></xs:enumeration>
                    <xs:enumeration value="m2"></xs:enumeration>
                    <xs:enumeration value="km2"></xs:enumeration>
                </xs:restriction>
            </xs:simpleType>
        </xs:attribute>
    </xs:complexType>
    <xs:simpleType name="currencyCodeType">
        <xs:restriction base="xs:string">
            <xs:minLength value="3"></xs:minLength>
            <xs:maxLength value="3"></xs:maxLength>
            <xs:pattern value="[A-Z]{3}"></xs:pattern>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="countryCodeType">
        <xs:restriction base="xs:string">
            <xs:minLength value="2"></xs:minLength>
            <xs:maxLength value="2"></xs:maxLength>
            <xs:pattern value="[A-Z]{2}"></xs:pattern>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="fullUrl">
        <xs:simpleContent>
            <xs:extension base="xs:anyURI">
                <xs:attribute type="urlTypes" name="type" use="optional"></xs:attribute>
                <xs:attribute type="xs:string" name="title" use="optional"></xs:attribute>
                <xs:attribute type="xs:string" name="label" use="optional"></xs:attribute>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
